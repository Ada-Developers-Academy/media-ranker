<table class="table table-hover">
  <tr>
    <th> Ranking </th>
    <th> Name </th>
    <th> Upvote </th>
  </tr>  
  <% @albums.order("rank DESC").each do |album| %>
    <tr>
      <td> Ranked: <%= album.rank %> </td>
      <td> <%= link_to album.name, album %> </td>
      <td>
        <%= form_for album do |f| %>
          <%= f.hidden_field :upvote %>
          <%= f.submit "Upvote", class: "btn btn-default" %>  
        <% end %>
      </td>  
    </tr>  
  <% end %>  
</table>

<%= link_to "View All Media", root_path, class: "btn btn-default"%>
<%= link_to "Add an Album", new_album_path, class: "btn btn-default"  %>
